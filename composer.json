{
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.5",
    "psr/container": "^2.0",
    "symfony/mailer": "^6.0",
    "doctrine/dbal": "^3.6",
    "doctrine/orm": "^2.16",
    "symfony/cache": "^6.3",
    "doctrine/migrations": "^3.6",
    "illuminate/database": "^10.24",
    "illuminate/events": "^10.24",
    "twig/twig": "^3.7",
    "twig/intl-extra": "^3.7",
    "slim/slim": "4.*",
    "slim/psr7": "^1.6",
    "slim/twig-view": "^3.3",
    "php-di/php-di": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
